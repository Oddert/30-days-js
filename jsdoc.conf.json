

{
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"template": "node_modules/better-docs"
	},
	"source": {
			"include": ["./"],
			"exclude": ["node_modules/"],
			"includePattern": ".+\\.js(doc|x)?$",
			"excludePattern": "(^|\\/|\\\\)_"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"plugins": [
		"plugins/markdown",
		"plugins/summarize",
		"node_modules/better-docs/category"
	],
	"templates": {
		"better-docs": {
				"name": "#JS30 | Robyn F H Veitch"
		}
	}
}

